{
  "name": "file-type-lint",
  "version": "2.0.0",
  "description": "Lint file types based on content",
  "keywords": [
    "mime",
    "file",
    "type",
    "archive",
    "image",
    "img",
    "pic",
    "picture",
    "flash",
    "photo",
    "video",
    "detect",
    "check",
    "is",
    "exif",
    "exe",
    "binary",
    "buffer",
    "uint8array",
    "jpg",
    "png",
    "gif",
    "webp",
    "flif",
    "cr2",
    "tif",
    "bmp",
    "jxr",
    "psd",
    "zip",
    "tar",
    "rar",
    "gz",
    "bz2",
    "7z",
    "dmg",
    "mp2",
    "mp4",
    "m4v",
    "mid",
    "mkv",
    "webm",
    "mov",
    "avi",
    "mpg",
    "mp3",
    "m4a",
    "ogg",
    "opus",
    "flac",
    "qcp",
    "wav",
    "amr",
    "pdf",
    "epub",
    "mobi",
    "swf",
    "rtf",
    "woff",
    "woff2",
    "eot",
    "ttf",
    "otf",
    "ico",
    "flv",
    "ps",
    "xz",
    "sqlite",
    "xpi",
    "cab",
    "deb",
    "ar",
    "rpm",
    "Z",
    "lz",
    "msi",
    "mxf",
    "mts",
    "wasm",
    "webassembly",
    "blend",
    "bpg",
    "docx",
    "pptx",
    "xlsx",
    "3gp",
    "ogm",
    "jp2",
    "ogx",
    "ogv",
    "spx",
    "jpm",
    "jpx",
    "mj2",
    "aif",
    "odt",
    "ods",
    "odp",
    "heic",
    "xml",
    "svg",
    "yml",
    "yaml",
    "check",
    "lint",
    "linter"
  ],
  "author": "Itgalaxy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/itgalaxy/file-type-lint.git"
  },
  "homepage": "https://github.com/itgalaxy/file-type-lint",
  "bugs": "https://github.com/itgalaxy/file-type-lint/issues",
  "main": "index.js",
  "dependencies": {
    "meow": "^5.0.0",
    "get-stdin": "^6.0.0",
    "ignore": "^5.0.3",
    "globby": "^7.0.0",
    "file-type": "^10.3.0",
    "sax": "^1.2.4",
    "xml2js": "^0.4.19",
    "js-yaml": "^3.12.0",
    "pify": "^4.0.1"
  },
  "devDependencies": {
    "del-cli": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-itgalaxy": "^86.0.0",
    "eslint-plugin-jest": "^21.26.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-lodash": "^4.0.0",
    "eslint-plugin-markdown": "^1.0.0-rc.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-unicorn": "^6.0.1",
    "jest": "^23.6.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.0",
    "npm-run-all": "^4.0.1",
    "npmpub": "^4.0.1",
    "prettier": "^1.5.2",
    "remark-cli": "^6.0.0",
    "remark-preset-lint-itgalaxy": "^13.0.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint:prettify": "prettier --list-different '{src,bin}/**/*.{js,mjs,jsx,md}' '*.{js,mjs,jsx,md}'",
    "lint:js": "eslint . --cache --report-unused-disable-directives --ignore-path .gitignore --ext 'js,.mjs,.jsx,.md'",
    "lint:md": "remark . -i .gitignore -f -q",
    "lint": "npm-run-all -l -p lint:**",
    "prettify": "npm run lint:prettify -- --write",
    "fix:js": "npm run lint:js -- --fix",
    "fix": "npm-run-all -l prettify -p 'fix:**'",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "release": "npmpub"
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "script"
    },
    "extends": [
      "plugin:itgalaxy/jest",
      "plugin:itgalaxy/esnext",
      "plugin:itgalaxy/node",
      "plugin:itgalaxy/markdown"
    ],
    "overrides": [
      {
        "rules": {
          "import/no-unresolved": [
            "error",
            {
              "ignore": [
                "file-type-lint"
              ]
            }
          ]
        },
        "files": [
          "**/*.md"
        ]
      }
    ],
    "root": true
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-itgalaxy"
    ]
  },
  "engines": {
    "node": ">= 6.9.0 || >= 8.9.0"
  },
  "bin": "bin/file-type-lint.js",
  "files": [
    "bin",
    "src",
    "!**/__tests__"
  ]
}
